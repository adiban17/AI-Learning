   "source": [
    "from nltk.stem import WordNetLemmatizer\n",
    "lemmatizer=WordNetLemmatizer()\n",
    "lemmatizer.lemmatize('doing', pos='v')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "1b5c5784",
   "metadata": {},
